{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && yarn run tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect=127.0.0.1:9223 --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/src/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "init": "node node_modules/ts-node/dist/bin.js node_modules/knex/bin/cli migrate:latest --esm && node node_modules/ts-node/dist/bin.js node_modules/knex/bin/cli seed:run --esm && pm2-runtime start processes.config.js --env test"
  },
